{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "windmill-blocks/button-group",
	"version": "0.1.0",
	"title": "Button Group",
	"category": "windmill-blocks",
	"icon": "button",
	"description": "Group of one or more customizable buttons",
	"keywords": ["link", "windmill"],
	"example": {
		"innerBlocks": [
			{
				"name": "windmill-blocks/button",
				"innerBlocks": [
					{
						"name": "core/paragraph",
						"attributes": {
							"content": "Find out more"
						}
					}
				]
			},
			{
				"name": "windmill-blocks/button",
				"innerBlocks": [
					{
						"name": "core/paragraph",
						"attributes": {
							"content": "Contact us"
						}
					}
				],
				"attributes": {
					"style": {
						"color": {
							"background": "transparent",
							"text": "black"
						},
						"border": {
							"width": 1,
							"style": "solid",
							"color": "black"
						}
					}
				}
			}
		]
	},
	"supports": {
		"html": false,
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textColumns": true,
			"textIndent": true,
			"__experimentalFontFamily": true,
			"__experimentalTextDecoration": true,
			"__experimentalFontStyle": true,
			"__experimentalFontWeight": true,
			"__experimentalLetterSpacing": true,
			"__experimentalTextTransform": true,
			"__experimentalWritingMode": true,
			"fitText": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		},
		"layout": {
			"default": {
				"type": "flex",
				"flexWrap": "nowrap",
				"justifyContent": "left",
				"orientation": "horizontal",
				"verticalAlignment": "center"
			},
			"allowJustification": true,
			"allowOrientation": true,
			"allowSizingOnChildren": true,
			"allowVerticalAlignment": true,
			"allowWrap": true,
			"allowEditing": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"align": true,
		"alignWide": true,
		"position": {
			"sticky": true
		}
	},
	"textdomain": "windmill-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
