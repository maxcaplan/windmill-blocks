{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "windmill-blocks/icon",
	"parent": ["windmill-blocks/icon-group"],
	"version": "0.1.0",
	"title": "Icon",
	"category": "windmill-blocks",
	"description": "A small image to convey a meaning",
	"keywords": ["image", "windmill"],
	"attributes": {
		"size": {
			"type": "string"
		},
		"icon": {
			"type": "string"
		},
		"ariaLabel": {
			"type": "string"
		}
	},
	"supports": {
		"spacing": {
			"margin": true,
			"padding": true
		},
		"color": {
			"background": true,
			"text": true,
			"gradients": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		}
	},
	"textdomain": "windmill-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
